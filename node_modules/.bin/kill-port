#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ldilone/projects/vue2-demo/node_modules/.pnpm/kill-port@1.6.1/node_modules/kill-port/node_modules:/Users/ldilone/projects/vue2-demo/node_modules/.pnpm/kill-port@1.6.1/node_modules:/Users/ldilone/projects/vue2-demo/node_modules/.pnpm/node_modules:/Users/ldilone/projects/vue2-demo/node_modules:/Users/ldilone/projects/node_modules:/Users/ldilone/node_modules:/Users/node_modules:/node_modules:/Users/ldilone/projects/vue2-demo/node_modules/kill-port/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/ldilone/projects/vue2-demo/node_modules/.pnpm/kill-port@1.6.1/node_modules/kill-port/node_modules:/Users/ldilone/projects/vue2-demo/node_modules/.pnpm/kill-port@1.6.1/node_modules:/Users/ldilone/projects/vue2-demo/node_modules/.pnpm/node_modules:/Users/ldilone/projects/vue2-demo/node_modules:/Users/ldilone/projects/node_modules:/Users/ldilone/node_modules:/Users/node_modules:/node_modules:/Users/ldilone/projects/vue2-demo/node_modules/kill-port/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../kill-port/cli.js" "$@"
else
  exec node  "$basedir/../kill-port/cli.js" "$@"
fi
